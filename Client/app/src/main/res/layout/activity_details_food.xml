<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_details_food"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="homework.com.bingeeatingproject.DetailsFoodActivity">

    <Switch
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/switch1_fdbinge"
        android:layout_alignBaseline="@+id/textView9"
        android:layout_alignBottom="@+id/textView9"
        android:layout_alignEnd="@+id/textView_fdtime" />

    <Switch
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/switch_fdvomiting"
        android:layout_alignBaseline="@+id/textView10"
        android:layout_alignBottom="@+id/textView10"
        android:layout_alignStart="@+id/switch1_fdbinge" />

    <Switch
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/switch_fdlexative"
        android:layout_alignBaseline="@+id/textView11"
        android:layout_alignBottom="@+id/textView11"
        android:layout_alignStart="@+id/switch_fdvomiting" />

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView_fdnotes"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="117dp"
        android:layout_alignStart="@+id/textView11"
        android:layout_alignEnd="@+id/textView_fdfood" />

    <TextView
        android:text="@string/lexative"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView11"
        android:layout_alignParentStart="true"
        android:layout_marginStart="34dp"
        android:paddingTop="40dp"
        android:layout_below="@+id/textView10" />

    <TextView
        android:text="@string/vomiting"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView10"
        android:paddingTop="20dp"
        android:layout_alignStart="@+id/textView11"
        android:layout_below="@+id/switch1_fdbinge" />

    <TextView
        android:text="@string/binge"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView9"
        android:layout_centerVertical="true"
        android:layout_alignStart="@+id/textView10" />

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView_fdfood"
        android:layout_marginBottom="23dp"
        android:layout_above="@+id/switch1_fdbinge"
        android:layout_alignStart="@+id/textView9" />

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView_fdplace"
        android:layout_above="@+id/textView_fdfood"
        android:layout_alignStart="@+id/textView_fdfood"
        android:layout_marginBottom="34dp" />

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="53dp"
        android:id="@+id/textView_fdtime"
        android:layout_above="@+id/textView_fdplace"
        android:layout_toEndOf="@+id/textView11"
        android:layout_marginBottom="22dp" />

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView_fddate"
        android:layout_alignBaseline="@+id/textView_fdtime"
        android:layout_alignBottom="@+id/textView_fdtime"
        android:layout_alignStart="@+id/textView_fdplace" />

    <ImageView
        android:layout_width="100dp"
        android:layout_height="100dp"
        app:srcCompat="@color/cardview_light_background"
        android:layout_toEndOf="@+id/textView10"
        android:layout_marginStart="16dp"
        android:id="@+id/imageView5_fad"
        android:layout_alignParentTop="true" />

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/textView_fdfood"
        android:layout_alignEnd="@+id/imageView5_fad"
        android:id="@+id/textView_fddrink" />

</RelativeLayout>
